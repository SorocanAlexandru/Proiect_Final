<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Proiect_Final.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proiect_Final"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
 Title="Principala"
 ContentTemplate="{DataTemplate local:MainPage}"
 />
        <ShellContent
 Title="Descriere Restaurante"
 ContentTemplate="{DataTemplate local:Restaurante}"
 />
        <ShellContent
 Title="Adrese Restaurante"
 ContentTemplate="{DataTemplate local:RestaurantEntryPage}"
 />
        
        <ShellContent
 Title="Rezervari Restaurante"
 ContentTemplate="{DataTemplate local:ListaEntryRestaurante}"
 />
    </TabBar>

</Shell>
